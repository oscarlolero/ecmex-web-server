<div class="courses">
    <div class="table100 ver3 m-t-10">
        <table data-vertable="ver3">
            <thead>
            <tr class="row100 head">
                <th class="column100 column2">Usuario</th>
                <th class="column100 columnAction">Datos</th>
            </tr>
            </thead>
            <tbody>
            <% data.forEach(function(user) { %>
                <tr class="row100">
                    <td class="column100 column2"><%= user.username %></td>
                    <td class="column100 columnAction"><button type="button" class="btn btn-outline-info btn-sm"
                      data-adress="<%= user.bill.adress %>"
                      data-city="<%= user.bill.city %>"
                      data-cp="<%= user.bill.cp %>"
                      data-email="<%= user.bill.email %>"
                      data-first_name="<%= user.bill.first_name %>"
                      data-last_name="<%= user.bill.last_name %>"
                      data-phone="<%= user.bill.phone %>"
                      data-rfc="<%= user.bill.rfc %>">Datos</button></td>
                </tr>
            <% }) %>
            </tbody>
        </table>
    </div>
</div>