<!DOCTYPE html>
<html lang="en">

<head>
    <title>Administraci√≥n</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="images/icons/favicon.ico"/>
    <link rel="stylesheet" type="text/css" href="js/admin/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="fonts/font-awesome-5.5.0/css/all.css">
    <link rel="stylesheet" type="text/css" href="js/admin/animate/animate.css">
    <link rel="stylesheet" type="text/css" href="js/admin/select2/select2.min.css">
    <link rel="stylesheet" type="text/css" href="js/admin/select2/select2.min.css">
    <link href="https://fonts.googleapis.com/css?family=Lato:100,300,300i,400" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="css/admin/univ.css">
    <link rel="stylesheet" type="text/css" href="css/admin/util.css">
</head>

<body>
<% include partials/nav %>
<div class="courses js_global_container" data-type="courses">
    <div class="table100 ver3 m-t-10">
        <table data-vertable="ver3">
            <thead>
            <tr class="row100 head">
                <th class="column100 column1">Code <span title="Add course" data-toggle="modal" data-target="#addModal"><i class="fas fa-plus-circle"></i></span></th>
                <th class="column100 column2">Course</th>
                <th class="column100 columnAction">Action</th>
            </tr>
            </thead>
            <tbody>
            <tr class="row100">
                <td class="column100 column1">

                </td>
                <td class="column100 column2">

                </td>
                <td class="column100 columnAction">
                    <!--                        <span data-process="edit-" title="Edit course" data-toggle="modal" data-target="#editModal"><i class="fas fa-edit"></i></span>-->
                    <!--                        <span data-process="delete-" title="Delete course"><i class="fas fa-minus-circle"></i></span>-->
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>
<!-- Insert modal -->
<div class="insert-modal" data-js_edit_id="-1">
    <div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
         aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Add course</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body m-t-20">
                    <div class="wrap-input100 validate-input m-b-35" data-validate="Password missing">
                        <input class="input100" id="field1" type="text" maxlength="5">
                        <span class="focus-input100" data-placeholder="Code of course"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Password missing">
                        <input class="input100" id="field2" type="text">
                        <span class="focus-input100" data-placeholder="Name of course"></span>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-add btn-validate">Add</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Edit modal -->
<div class="edit-modal">
    <div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
         aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Edit course</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body m-t-20">
                    <div class="wrap-input100 validate-input" data-validate="Password missing">
                        <input class="input100" id="efield1" type="text" maxlength="5">
                        <span class="focus-input100" data-placeholder="Code of course"></span>
                    </div>
                </div>
                <div class="modal-body m-t-20">
                    <div class="wrap-input100 validate-input" data-validate="Password missing">
                        <input class="input100" id="efield2" type="text">
                        <span class="focus-input100" data-placeholder="Name of course"></span>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-edit">Edit</button>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="js/admin/jquery/jquery-3.2.1.min.js"></script>
<script src="js/admin/bootstrap/js/popper.js"></script>
<script src="js/admin/bootstrap/js/bootstrap.min.js"></script>
<script src="js/admin/select2/select2.min.js"></script>
<script src="js/admin/xlsx/xlsx.full.min.js"></script>
<script src="js/admin/filesaver/FileSaver.min.js"></script>
<script src="js/edit.js"></script>

</body>

</html>